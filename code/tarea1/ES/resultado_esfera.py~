import numpy as np
import matplotlib.pyplot as plt
from scipy.stats import norm

data = [0.10903516394412024,0.056775547866147136,0.23882242490005728,0.1633401519555946,0.054641355077869366,0.30423607645372197,0.18309962404809044,0.13766930102474184,0.016509574451649855,0.14063635112803352,0.05396577951454969,0.0490230981440093,0.30140435608731586,0.03844676659335111,0.08986104349186519,0.09068716125469933,0.0988478737749696,0.18328953107656787,0.010867412739379967,0.2117040782026321,0.20307142238763382,0.20099311536409648,0.031337542754326986,0.059038710848325016,0.033163023056060115,0.05638485297764738,0.17786637848631237,0.05323675019450448,0.005118275346257237,0.0018761635613861942]

mean = np.mean(data)
std_dev = np.std(data)

# Create a range of x values
x = np.linspace(mean - 3 * std_dev, mean + 3 * std_dev, 100)

# Use the normal distribution to calculate y values
y = norm.pdf(x, mean, std_dev)

# Plot the data
plt.plot(x, y)
plt.xlabel('Data')
plt.ylabel('Probability density')
plt.savefig('esfera.png')
